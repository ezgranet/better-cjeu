{
  "manifest_version": 3,
 
  "name": "Better CJEU",
  "version": "1.0",
  "description": "The extension changes the typography on EU websites to make it more visually appealing.",
  "icons": { "19": "icon19.png",
           "38": "icon38.png",
          "128": "icon128.png" },
  "content_scripts": [{
    "css": ["styles.css"],
    "matches": ["https://eur-lex.europa.eu/*", "https://www.curia.europa.eu/*", "https://curia.europa.eu/*"]
     }]
}
